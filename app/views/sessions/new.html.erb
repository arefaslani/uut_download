<%= form_for :session, url: sessions_url(redirect_url: params['redirect_url']) do |f| %>
  <p>
    <%= f.label :student_number %>
    <%= f.text_field :student_number %>
  </p>

  <p>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </p>

  <p>
    <%= f.submit 'Log in' %>
  </p>
<% end %>